	       etirer(facteur:No+Cr f#5) etirer(facteur:Dc g5)
	       etirer(facteur:Dc f#5) etirer(facteur:Bl e5)
	      ]
   Mesure23 = [
	       etirer(facteur:Dc d5) etirer(facteur:Dc e5)
	       etirer(facteur:Bl+No f#5) etirer(facteur:Cr silence)
	      ]
   Mesure24 = [
	       etirer(facteur:Dc f#5) etirer(facteur:Dc g5)
	       etirer(facteur:No+Cr a5) etirer(facteur:Cr silence)
	       etirer(facteur:Cr e5) etirer(facteur:No silence)
	      ]
   Mesure25 = [
	       etirer(facteur:Dc d5) etirer(facteur:Dc e5)
	       etirer(facteur:Dc f#5) etirer(facteur:No+Cr+Dc silence)
	       etirer(facteur:Cr f#5) etirer(facteur:Cr silence)
	       etirer(facteur:Cr f#5)
	      ]
   Mesure26 = [
	       etirer(facteur:Dc e5) etirer(facteur:Dc f#5)
	       etirer(facteur:Dc g#5) etirer(facteur:Cr+Dc silence)
	       etirer(facteur:Dc g#5) etirer(facteur:Cr silence)
	       etirer(facteur:Cr g#5) etirer(facteur:Cr silence)
	       etirer(facteur:Cr g#5)
	      ]
   Mesure27 = [
	       etirer(facteur:Dc f#5) etirer(facteur:Dc g#5)
	       etirer(facteur:Dc a#5) etirer(facteur:Cr+Dc silence)
	       etirer(facteur:Dc a#5) etirer(facteur:Cr silence)
	       etirer(facteur:Cr a#5) etirer(facteur:Cr silence)
	       etirer(facteur:Cr a#5)
	      ]
   Mesure28 = [ etirer(facteur:Ro a#5) ]
   Mesure29 = [ etirer(facteur:Ro a4) ]
   Mesure30 = [ etirer(facteur:Ro b4) ]
   Mesure31 = [
	       etirer(facteur:Bl g4) etirer(facteur:No g4)
	       etirer(facteur:No f#4)
	      ]
   Mesure32 = [ etirer(facteur:Ro e4) ]
   

   % Ceci n'est pas une musique
   Partition = [Mesure1 Mesure2 Mesure3 Mesure4
		Mesure5 Mesure6 Mesure7 Mesure8 Mesure9 Mesure10 Mesure11 Mesure12
		Mesure13 Mesure14 Mesure15 Mesure16 Mesure17
		Mesure18 Mesure19 Mesure20 Mesure21 Mesure22 Mesure23
		Mesure24 Mesure25 Mesure26
		Mesure27 Mesure28 Mesure29 Mesure30 Mesure31 Mesure32]
in
   % Ceci est une musique :-)
   [partition(Partition)]
end

